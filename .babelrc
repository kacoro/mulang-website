{
  "env": {
    "test": {
      "presets": [
        [
         "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "node": "current",
              "browsers": [
                "ie 9",
                "ios 9",
                "last 2 chrome versions",
                "last 2 edge versions",
                "last 2 firefox versions",
              ]
            }
          }
        ]
      ],
      "plugins": [
        [
          "component",
          {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-chalk"
          }
        ]]
    }
  }
}
